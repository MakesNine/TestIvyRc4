{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "TestIvyRc4": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/TestIvyRc4",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/amcharts/dist/amcharts/plugins/export/export.css",
              "node_modules/ammaps/dist/ammap/ammap.css",
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/amcharts/dist/amcharts/amcharts.js",
              "node_modules/ammaps/dist/ammap/plugins/export/export.min.js",
              "node_modules/ammaps/dist/ammap/plugins/export/libs/pdfmake/pdfmake.js",
              "node_modules/ammaps/dist/ammap/plugins/export/libs/jszip/jszip.js",
              "node_modules/ammaps/dist/ammap/plugins/export/libs/fabric.js/fabric.js",
              "node_modules/ammaps/dist/ammap/plugins/export/libs/FileSaver.js/FileSaver.js",
              "node_modules/ammaps/dist/ammap/plugins/responsive/responsive.js",
              "node_modules/amcharts/dist/amcharts/themes/light.js",
              "node_modules/ammaps/dist/ammap/ammap.js",
              "node_modules/ammaps/dist/ammap/maps/js/worldlow.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "TestIvyRc4:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "TestIvyRc4:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "TestIvyRc4:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "TestIvyRc4:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "TestIvyRc4:serve:production"
            }
          }
        }
      }
    },
    "chart1": {
      "projectType": "library",
      "root": "projects/chart1",
      "sourceRoot": "projects/chart1/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/chart1/tsconfig.lib.json",
            "project": "projects/chart1/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/chart1/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/chart1/src/test.ts",
            "tsConfig": "projects/chart1/tsconfig.spec.json",
            "karmaConfig": "projects/chart1/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/chart1/tsconfig.lib.json",
              "projects/chart1/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "TestIvyRc4",
  "cli": {
    "analytics": "d5298c30-4b13-4adc-a23c-ca0e078cce0f"
  }
}